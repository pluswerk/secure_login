{
  "name": "pluswerk/secure-login",
  "license": "GPL-3.0+",
  "type": "typo3-cms-extension",
  "version": "2.0.0",
  "description": "+Pluswerk TYPO3 extension: Secure Login - brute-force security",
  "homepage": "https://www.pluswerk.ag",
  "support": {
    "source": "https://github.com/pluswerk/secure_login/",
    "issues": "https://github.com/pluswerk/secure_login/issues",
    "docs": "https://github.com/pluswerk/secure_login/blob/master/Readme.md"
  },
  "require": {
    "php": ">=7.2.0",
    "typo3/cms-core": ">=9.5, <10.4"
  },
  "require-dev": {
    "pluswerk/grumphp-config": "2.*"
  },
  "autoload": {
    "psr-4": {
      "Pluswerk\\SecureLogin\\": "Classes/"
    }
  },
  "replace": {
    "pluswerk/secure_login": "self.version",
    "typo3-ter/secure_login": "self.version",
    "typo3-ter/secure-login": "self.version"
  },
  "extra": {
    "pluswerk/grumphp-config": {
      "auto-setting": true
    },
    "grumphp": {
      "config-default-path": "vendor/pluswerk/grumphp-config/grumphp.yml"
    }
  }
}
