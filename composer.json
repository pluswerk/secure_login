{
  "name": "pluswerk/secure-login",
  "license": "GPL-3.0+",
  "type": "typo3-cms-extension",
  "version": "1.0.0",
  "description": "pluswerk: Security for authentications",
  "homepage": "https://github.com/pluswerk/secure_login",
  "support": {
    "source": "https://github.com/pluswerk/secure_login/issues"
  },
  "require": {
    "php": ">=5.6.0"
  },
  "require-dev": {
    "pluswerk/grumphp-config": "2.*"
  },
  "autoload": {
    "psr-4": {
      "Pluswerk\\SecureLogin\\": "Classes/"
    }
  },
  "replace": {
    "secure_login": "self.version",
    "typo3-ter/secure_login": "self.version",
    "typo3-ter/secure-login": "self.version"
  },
  "extra": {
    "pluswerk/grumphp-config": {
      "auto-setting": true
    },
    "grumphp": {
      "config-default-path": "vendor/pluswerk/grumphp-config/grumphp.yml"
    }
  }
}
